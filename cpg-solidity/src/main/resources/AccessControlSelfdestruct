match p=(:FunctionDeclaration)-[:EOG|INVOKES*]->(c:CallExpression)-[:EOG|INVOKES*]->(last)
        where toUpper(c.localName) in ['SELFDESTRUCT' , 'SUICIDE']
        and not exists ((last)-[:EOG|INVOKES]->()) 
        and not 'Rollback' in labels(last) 
        and not exists{
            ({code : 'msg.sender'})-[:DFG*]->(n)<-[:DFG*]-(:FieldDeclaration)  // Here we might have to consider refers to to Field declarations and not strait data flows
            where n in nodes(p)
            and exists{
                alt=(n)-[:EOG|INVOKES*]->(t) where not 'Rollback' in labels(t) and not c in nodes(alt)
            }
        }
return distinct c as call, c.startLine as sline, c.endLine as eline, c.startColumn as scol, c.endColumn as ecol, c.artifact as file